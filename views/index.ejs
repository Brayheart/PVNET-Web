<!DOCTYPE html>
<html>
<head>
  <title>PVNet Login</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <script>
    function inputUser(){
        var First = document.getElementsByName('First')[0].value();
        window.location.href = "/ClassesDisplay?First=" + First;
    }

  </script>
</head>
<body>
<video autoplay muted loop id="myVideo">
  <source src="/Videoes/Meeting.MP4" type="video/mp4">
</video>

<div class="container">
  <div class="image-container">
    <img src="/images/pvnet.jpg" alt="Avatar" class="round-image">
  </div>
  <div class="form-container">
    <%- include Components/Login.ejs %>
    <div class="buttoms">
      <div class="container-login100-form-btn">
        <button class="login100-form-btn" form = "info" value = "login" onclick="inputUser()">
          <span class="login100-form-btn-text">Login</span>
        </button>
      </div>
      <div class="container-login100-form-btn">

        <button class="login100-form-btn" type = "button" value = "signout" onclick="window.open('/views/components/Signout.ejs', '_blank');">

          <span class="login100-form-btn-text">Signout</span>
        </button>

      </div>
    </div>



  </div>

  <script src="javascripts/main.js"></script>
  <script src="stylesheets/jquery/jquery-3.2.1.min.js"></script>
</body>
</html>
