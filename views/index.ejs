  <link rel='stylesheet' href='/stylesheets/style.css' />
  <script>
      function Edit(id) {
          window.location.href = "/userEdit?Class_ID=" + id;
      }

      function Delete(id) {
          var rs = confirm('Confirm to delete?');
          if (rs) {
              window.location.href = "/userDelete?Class_ID=" + id;
          }
      }

      function Search(userid) {

          window.location.href = "/InstructorSearchDisplay?Instructor=" + userid;
      }

  </script>
</head>

